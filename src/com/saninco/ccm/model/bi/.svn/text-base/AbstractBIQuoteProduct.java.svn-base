package com.saninco.ccm.model.bi;

public abstract class AbstractBIQuoteProduct implements java.io.Serializable  {
	/**
	 * 
	 */
	private static final long serialVersionUID = -6411063580802618174L;
	private Integer id;
	private String productName;
	private String mediaLabel;
	private String functionLabel;
	private String generationLabel;

	// Constructors

	/** default constructor */
	public AbstractBIQuoteProduct() {
	}

	/** full constructor */
	public AbstractBIQuoteProduct(String productName, String mediaLabel,
			String functionLabel, String generationLabel) {
		this.productName = productName;
		this.mediaLabel = mediaLabel;
		this.functionLabel = functionLabel;
		this.generationLabel = generationLabel;
	}

	// Property accessors

	public Integer getId() {
		return this.id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getProductName() {
		return this.productName;
	}

	public void setProductName(String productName) {
		this.productName = productName;
	}

	public String getMediaLabel() {
		return this.mediaLabel;
	}

	public void setMediaLabel(String mediaLabel) {
		this.mediaLabel = mediaLabel;
	}

	public String getFunctionLabel() {
		return this.functionLabel;
	}

	public void setFunctionLabel(String functionLabel) {
		this.functionLabel = functionLabel;
	}

	public String getGenerationLabel() {
		return this.generationLabel;
	}

	public void setGenerationLabel(String generationLabel) {
		this.generationLabel = generationLabel;
	}
}
