package com.saninco.ccm.model;

import java.math.BigDecimal;
import java.util.Date;

public class AbstractTargetResult implements java.io.Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private Integer id;
	private Integer targetId;
	private String period;
	private Integer allowedPeriodShift;
	private Double actualAmount;
	private Double actualChangeAmount;
	private BigDecimal actualChangePercentage;
	private BigDecimal actualVariationPercentage;
	private String resultFlag;
	private String remark;
	private String recActiveFlag	;
	private Date createdTimestamp	;
	private Integer createdBy	;
	private Date modifiedTimestamp	;
	private Integer modifiedBy;
	
	
	
	public AbstractTargetResult() {
	}

	public AbstractTargetResult(Integer id, Integer targetId, String period,
			Integer allowedPeriodShift, Double actualAmount,
			Double actualChangeAmount, BigDecimal actualChangePercentage,
			BigDecimal actualVariationPercentage, String resultFlag,
			String remark, String recActiveFlag, Date createdTimestamp,
			Integer createdBy, Date modifiedTimestamp, Integer modifiedBy) {
		super();
		this.id = id;
		this.targetId = targetId;
		this.period = period;
		this.allowedPeriodShift = allowedPeriodShift;
		this.actualAmount = actualAmount;
		this.actualChangeAmount = actualChangeAmount;
		this.actualChangePercentage = actualChangePercentage;
		this.actualVariationPercentage = actualVariationPercentage;
		this.resultFlag = resultFlag;
		this.remark = remark;
		this.recActiveFlag = recActiveFlag;
		this.createdTimestamp = createdTimestamp;
		this.createdBy = createdBy;
		this.modifiedTimestamp = modifiedTimestamp;
		this.modifiedBy = modifiedBy;
	}

	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getTargetId() {
		return targetId;
	}
	public void setTargetId(Integer targetId) {
		this.targetId = targetId;
	}

	public String getPeriod() {
		return period;
	}

	public void setPeriod(String period) {
		this.period = period;
	}

	public Integer getAllowedPeriodShift() {
		return allowedPeriodShift;
	}
	public void setAllowedPeriodShift(Integer allowedPeriodShift) {
		this.allowedPeriodShift = allowedPeriodShift;
	}
	public Double getActualAmount() {
		return actualAmount;
	}
	public void setActualAmount(Double actualAmount) {
		this.actualAmount = actualAmount;
	}
	public Double getActualChangeAmount() {
		return actualChangeAmount;
	}
	public void setActualChangeAmount(Double actualChangeAmount) {
		this.actualChangeAmount = actualChangeAmount;
	}
	public BigDecimal getActualChangePercentage() {
		return actualChangePercentage;
	}
	public void setActualChangePercentage(BigDecimal actualChangePercentage) {
		this.actualChangePercentage = actualChangePercentage;
	}
	public BigDecimal getActualVariationPercentage() {
		return actualVariationPercentage;
	}
	public void setActualVariationPercentage(BigDecimal actualVariationPercentage) {
		this.actualVariationPercentage = actualVariationPercentage;
	}
	public String getResultFlag() {
		return resultFlag;
	}
	public void setResultFlag(String resultFlag) {
		this.resultFlag = resultFlag;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getRecActiveFlag() {
		return recActiveFlag;
	}

	public void setRecActiveFlag(String recActiveFlag) {
		this.recActiveFlag = recActiveFlag;
	}

	public Integer getCreatedBy() {
		return createdBy;
	}

	public void setCreatedBy(Integer createdBy) {
		this.createdBy = createdBy;
	}

	public Integer getModifiedBy() {
		return modifiedBy;
	}

	public void setModifiedBy(Integer modifiedBy) {
		this.modifiedBy = modifiedBy;
	}

	public Date getCreatedTimestamp() {
		return createdTimestamp;
	}

	public void setCreatedTimestamp(Date createdTimestamp) {
		this.createdTimestamp = createdTimestamp;
	}

	public Date getModifiedTimestamp() {
		return modifiedTimestamp;
	}

	public void setModifiedTimestamp(Date modifiedTimestamp) {
		this.modifiedTimestamp = modifiedTimestamp;
	}
	
}
