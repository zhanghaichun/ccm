package com.saninco.ccm.model;

import java.util.Date;
import java.util.Date;

public class AbstractTariff implements java.io.Serializable{
	private Integer id;
	private String recent_rate_change;
	private Date effective_date;
	private Integer attachment_point_id;
	
	public AbstractTariff() {
	}
	public AbstractTariff(Integer id,String recent_rate_change,Date effective_date,Integer attachment_point_id){
		super();
		this.id = id;
		this.recent_rate_change = recent_rate_change;
		this.effective_date= effective_date;
		this.attachment_point_id = attachment_point_id;
		
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getRecent_rate_change() {
		return recent_rate_change;
	}
	public void setRecent_rate_change(String recentRateChange) {
		recent_rate_change = recentRateChange;
	}
	public Date getEffective_date() {
		return effective_date;
	}
	public void setEffective_date(Date effectiveDate) {
		effective_date = effectiveDate;
	}
	public Integer getAttachment_point_id() {
		return attachment_point_id;
	}
	public void setAttachment_point_id(Integer attachmentPointId) {
		attachment_point_id = attachmentPointId;
	}
	
}
