.InSePoput{
	width:280px;
}
.InSePoput .ISPtop{
	width:280px;
	height:17px;
	overflow:hidden;
	background:url(images/InSePoput_top.png);
}

.InSePoput .ISPcon{
	width:280px;
	
	
}

.InSePoput .ISPcon .ISPleft{
	width:20px;
	height:61px;
	background:url(images/InSePoput_icon.png);
	float:left;
}

.InSePoput .ISPcon .ISPright{
	width:281px;
	background:url(images/InSePoput_con.png) repeat scroll 0 0;
}

.InSePoput .ISPcon .ISPright table{
	/*width:97%;*/
}

.InSePoput .ISPcon .ISPright tr{
	height:25px;
}

.InSePoput .ISPcon .ISPright td{
	padding:0 5px;
	line-height:25px;
}

.InSePoput .ISPdown{
	width:280px;
	height:20px;
	overflow:hidden;
	background:url(images/InSePoput_down.png);
}


.InDePoput{
	width:235px;
}

.InDePoput .IDPtop{
	width:235px;
	height:22px;
	background:url(images/InDePop_top.png);
}

.InDePoput .IDPcon{
	overflow:hidden;
	width:235px;
	background:url(images/InDePop_con.png);
}

.InDePoput .IDPcon .IDPleft{
	width:25px;
	height:36px;
	background:url(images/InDePop_icon.png);
	float:left;
}

.InDePoput .IDPcon .IDPright{
	width:190px;
	float:left;
	padding:0 10px;
}

.InDePoput .IDPcon .IDPright h3{
	margin-bottom:0px;
	line-height:25px;
	font-size:12px;
	color:#585858;
}

.InDePoput .IDPcon .IDPright p{
	margin-bottom:0px;
	line-height:25px;
	font-size:11px;
	color:#000000;
	font-weight:bold;
}

.InDePoput .IDPdown{
	width:235px;
	height:22px;
	overflow:hidden;
	background:url(images/InDePop_down.png);
}