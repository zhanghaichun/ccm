.x-sqs-list-item{font:normal 12px tahoma,arial,helvetica,sans-serif;padding:1px;border:1px solid #fff;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
.x-sqs-list .x-sqs-selected{border:1px dotted #f6f6f6!important;background:#d0d0d0;cursor:pointer;}
/*
* END Ext JS Library 2.0
*/
input[type=button],input[type=submit]{padding: 2px 6px;background-color:#fff;}

/* Dashboard List 最外层列表背景颜色 */
.dashboard-list-main-container.system-theme {
	background: #F6F6F6;
	border: 1px solid #d8c2ab;
}

/* section区域的轮廓样式 */
.dashboard-list-section.system-theme {
	background: #F6F6F6;
	border: 1px solid #d8c2ab;
}


/* 列表中下划线的颜色 */

.dashboard-list-main-container .dashboard-list .dashboard-list-item {
	border: 1px solid #d8c2ab;
	border-bottom: none;
}

.dashboard-list-main-container .dashboard-list .dashboard-list-item:last-child {
	border-bottom: 1px solid #d8c2ab;
}


/* Dashboard Page 主页的样式。 */

/* Dashboard Panel 的样式。 */
.dashboard-dashboards .quote-charts-panel-wrapper {
	background: #FDFDFD;
	border: 1px solid #E6DACE;
}

/* Add Dashboard Control的样式 */
.dashboard-default .add-new-container {
	background: #FDFDFD;
	border: 1px dashed #E6DACE;
}

/* Cancel button 和 Confirm button */
#dashboard-content .themes-cancel {
	color: #DFCEBB;
	border-color: #D9C3AC;
	background: #F6F6F6;
}

#dashboard-content .themes-cancel:hover {
	color: #F2951A;
	border-color: #F2951A;
}

#dashboard-content .themes-confirm {
	border-color: #F7CB7D;
	background: #F7CB7D;
}

#dashboard-content .themes-confirm:hover {
	border-color: #E7921B;
	background: #E7921B;
}


/* Panel 的下拉面板的样式 */
.quote-charts-panel .select-filter-panel {
	background: #FDFDFD;
	border: 1px solid #E6DACE;
}

/* Global Filter 的下拉列表选项 */
.global-controller-dropdown .global-controller-dropdown-title {
	background: #FDFDFD;
	border: 1px solid #E6DACE;
}

.global-controller-dropdown .global-controller-list-container {
	background: #FDFDFD;
	border: 1px solid #E6DACE;
	border-top: none;
}

/* Select Dashboard Type Panel */
.dashboard-default .select-dashboard-type-panel {
	background: #FDFDFD;
	border: 1px solid #E6DACE;
}
