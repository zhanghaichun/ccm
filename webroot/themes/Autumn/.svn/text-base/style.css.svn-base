/* Default Styles */
body { 
	font-size: 12px;
	margin: 0px 0px 0px 0px;
	font-family: Arial, Verdana, Helvetica, sans-serif;
	}
img {
	behavior:	url("/include/pngbehavior.htc");
}
.body { 

	font-size: 12px;
	}
	
table,td {
	font-size: 11px;
	}

p {
	MARGIN-TOP: 0px;
	MARGIN-BOTTOM: 10px;
	}
	
form {
	margin: 0px;
	}
 
h2, h1 {
	font-size: 14px; 
	font-family: Arial, Verdana, Helvetica, sans-serif;
	font-weight: bold; 
	margin-bottom: 0px;
	margin-top: 0px;
	}

h3, .monthHeaderH3, .schedulerTopDateCell {
	font-size: 13px; 
	font-family: Arial, Verdana, Helvetica, sans-serif;
	font-weight: bold; 
	margin-bottom: 5px;
	padding-top: 5px;
	}

.h3Row, .monthHeader, .schedulerTopDateCell, h4.tabDetailViewDL { 
	border-top: 1px solid;	
	padding: 0px 5px 5px 8px;
	margin-bottom: 0px;
	vertical-align: middle;
}
.h3Arrow {
background-repeat: no-repeat;
margin: 3px 4px 0 0;
}
.subpanelTabForm .h3Row {
	background: #f6f6f6;
	border-top: none;
	border-bottom: 1px solid;
	padding-left: 0;
	
}
.monthHeader {
	border-bottom: 1px solid;
}
.listViewButtons {
padding-left: 8px;
padding-top: 5px;
}
h4, h5, h5.listViewSubHeadS1 {
	font-family: Arial, Verdana, Helvetica, sans-serif;
	font-size: 12px; 
	font-weight: bold; 
	margin-bottom: 2px;
	padding-left: 4px;
	}
h4.dataLabel {
	margin-top: 0;
	padding-left: 0;
	padding-top: 0;
	font-family: Arial, Verdana, Helvetica, sans-serif;
	font-size: 12px; 
	font-weight: bold; 
	margin-bottom: 10px;
}

LI {
	LIST-STYLE-TYPE: square; 
	margin-left: 6px; 
	MARGIN-BOTTOM: 2px;
	}
	
UL {
	MARGIN-TOP: 2px;
	}
	
hr {
	height: 1px;
}
a:link, a:visited {
	text-decoration: underline;
	}
a:hover {
	text-decoration: underline;
	}
/* Form Styles */

.button {
	border: 1px solid;
	font-size: 11px;
	}
	
.buttonOn, .button:hover, .button:focus, .buttonOn:focus {
	font-size: 11px;
	border: 1px solid;
	background-color: #ffffff;
	font-weight: normal;
	}
.tabDetailView .button img{
margin-right: 5px;
margin-left: 5px;
}
#listViewPaginationButtons .button {
width: 25px;
}
.listView .button, .tabDetailView .button {
background-color: #eee;
}

input,select {
	border: 1px solid;
	font-size: 11px;
	}
	
input:focus,textarea:focus, select:focus { 
	 }
textarea {
	font-family: Arial, Verdana, Helvetica, sans-serif;
	font-size: 12px;
	border: 1px solid;
	}
.checkbox,.radio {
	border:0px;
	background-color: transparent;
	margin: 0px;
	vertical-align: bottom;
	}

	
/* Content Specific Styles */
	/* body text */
	.contentBox {
		border-top: 1px solid #999;
		border-bottom: 1px solid #999;
		padding: 0px 10px 0px 0px;
		margin: 10px 0 10px 0;
	}
	.teamNoticeBox {
		font-size: 12px;
		border-bottom: 2px solid;
		border-left: 1px solid;
		border-right: 1px solid;
		padding: 0px 10px 0px 10px;
		color: #000;
	}
	a.body:link, a.body:visited {
		text-decoration: underline;
		}
		
	a.body:hover {
		text-decoration: underline;
		}
	
