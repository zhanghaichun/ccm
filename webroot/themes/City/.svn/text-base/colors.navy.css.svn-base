#slideshow {
	
	width:100%;
	height:30px;
	position:relative;
}
#slideshow #slidesContainer {
  margin-left:65px;top:7;
  width:430px;
  height:30px;
  overflow:auto; /* allow scrollbar */
  position:relative;
  padding-left: 2px;
}
#slideshow #slidesContainer .slide {
  margin:0 auto;
  width:100%; /* reduce by 20 pixels of #slidesContainer to avoid horizontal scroll */
  height:30px;
}

/** 
 * Slideshow controls style rules.
 */
.control {
	
  display:block;
  width:25px;
 
  text-indent:-10000px;
  position:absolute;
  cursor: pointer;
}
#leftControl {
  width:25;
  height:25;
  top:4;
  left:15px;
  background:url(images/left.png);
}
#rightControl {
	width:25;
	height:25;
	top:4;
  left:460px;
  
  background:url(images/right.png);
}

/** 
 * Style rules for Demo page
 */

a {
  color: #000;
  /*font-weight:bold;*/
  text-decoration:none;
}
a:hover {
  text-decoration:underline;
}

#pageContainer {
	width:100%-20;
	*width:100%;
	
	
 /* background-color:#8FE1EF;*/
  
}
