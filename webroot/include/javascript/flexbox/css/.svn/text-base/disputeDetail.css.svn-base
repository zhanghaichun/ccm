
.wide {
	padding:10px 20px;
}
.disputeCheckbox {
	border-collapse:separate;
	border-spacing:0px 10px;
}
.disputeTab {
	padding-bottom: 20px;
	border-bottom: 1px solid #e6e6e6;
	margin-bottom:15px;
}
.disputeTab input {
	border:0px;
	width:100%;
	outline-style:none;
	font-family: Arial, Verdana, Helvetica, sans-serif;
	font-size: 13px;
	font-weight: bold;
	background-color:#f6f6f6;
}

.disputeTab .forward-to,
.disputeTab .reply-to {
	width: 25%;
    height: 25px;
    border-bottom: 1px solid #CCCCCC;
}

#textMessage {
	width:100%;
	outline-style:none;
	resize:none;
	border:0px;
	font-family: Arial, Verdana, Helvetica, sans-serif;
	font-size: 13px;
	font-weight: bold;
	background-color:#f6f6f6 !important;
}
